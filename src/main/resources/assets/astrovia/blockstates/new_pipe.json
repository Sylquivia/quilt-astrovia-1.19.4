{
  "multipart": [
    {
      "apply": {"model": "astrovia:block/new_pipe"}
    },
    {
      "apply": {"model": "astrovia:block/new_pipe_side"},
      "when": {"north": "true"}
    },
    {
      "apply": {"model": "astrovia:block/new_pipe_side", "y": 90},
      "when": {"east": "true"}
    },
    {
      "apply": {"model": "astrovia:block/new_pipe_side", "y": 180},
      "when": {"south": "true"}
    },
    {
      "apply": {"model": "astrovia:block/new_pipe_side", "y": 270},
      "when": {"west": "true"}
    }
  ]
}
