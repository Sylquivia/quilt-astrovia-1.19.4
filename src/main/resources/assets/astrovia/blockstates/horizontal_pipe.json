{
  "multipart": [
    {
      "apply": {"model": "astrovia:block/horizontal_pipe", "uvlock": true}
    },
    {
      "apply": {"model": "astrovia:block/horizontal_pipe_side", "uvlock": true},
      "when": {"north": "true"}
    },
    {
      "apply": {"model": "astrovia:block/horizontal_pipe_side", "uvlock": true, "y": 90},
      "when": {"east": "true"}
    },
    {
      "apply": {"model": "astrovia:block/horizontal_pipe_side", "uvlock": true, "y": 180},
      "when": {"south": "true"}
    },
    {
      "apply": {"model": "astrovia:block/horizontal_pipe_side", "uvlock": true, "y": 270},
      "when": {"west": "true"}
    }
  ]
}
