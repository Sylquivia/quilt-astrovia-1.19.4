{
  "multipart": [
    {
      "apply": {"model": "astrovia:block/directional_pipe", "uvlock": true}
    },

    {
      "when": {"north": "true"},
      "apply": {"model": "astrovia:block/directional_pipe_side", "uvlock": true}
    },
    {
      "when": {"east": "true"},
      "apply": {"model": "astrovia:block/directional_pipe_side", "uvlock": true, "y": 90}
    },
    {
      "when": {"south": "true"},
      "apply": {"model": "astrovia:block/directional_pipe_side", "uvlock": true, "y": 180}
    },
    {
      "when": {"west": "true"},
      "apply": {"model": "astrovia:block/directional_pipe_side", "uvlock": true, "y": 270}
    },
    {
      "when": {"down": "true"},
      "apply": {"model": "astrovia:block/directional_pipe_side", "uvlock": true, "x": 90}
    },
    {
      "when": {"up": "true"},
      "apply": {"model": "astrovia:block/directional_pipe_side", "uvlock": true, "x": 270}
    }
  ]
}
